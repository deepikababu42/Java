import java.util.*;
public class Main{
    public static void main(String[]args){
        Scanner Sc=new Scanner(System.in);
        String oct = Sc.nextLine();
        int a=Integer.parseInt(oct ,8);
       // int a= Sc.nextInt();
        //String hex = Integer.toHexString(a);
        System.out.println(a);
    }
}
