import java.util.*;
public class Main{
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
        double principalamount = sc.nextDouble();
        double rate = sc.nextDouble();
        int time = sc.nextInt();
        double SI = (principalamount*rate*time)/100;
        System.out.println(SI);
        
    }
}

        
