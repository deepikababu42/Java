import java.util.*;
public class Main{
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
         int n = sc.nextInt();
         int even =0;
         int odd =0;
         while(n>0){
             int d = n%10;
             if(d%2==0){
                 even++;
             }
             else{
                 odd++;
             }
             n=n/10;
         }
         if(even>0&&odd==0){
             System.out.println("All even");
         }
         else if(odd>0&&even==0){
             System.out.println("All odd");
         }
         else{
         System.out.println("Mixed");
         }
    }
    
}
