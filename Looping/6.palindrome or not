import java.util.*;
public class Main{
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int reverse =0;
        int temp =num;
        while(num>0){
            int digit = num%10;
            reverse=reverse*10+digit;
            num/=10;
        }
        if(reverse==temp){
            System.out.println("palindrome");
        }
        else{
            System.out.println("Not a palindrome");
        }
    }
}
