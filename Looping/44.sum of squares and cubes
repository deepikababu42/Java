import java.util.*;
public class Main{
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
         int n = sc.nextInt();
         int squaresum =0;
         int cubesum = 0;
         while(n>0){
             int d = n%10;
            // if(d%n==0){
                 squaresum+=d*d;
                 cubesum+=d*d*d;
             //}
             n=n/10;
         }
        
         System.out.println(squaresum+cubesum);
    }
    
}
