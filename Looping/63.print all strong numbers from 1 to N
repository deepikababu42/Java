import java.util.*;
public class Main{
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        for (int i = 1; i <= n; i++) {
            int temp = i;
            int sum = 0;
            while (temp > 0) {
                int d = temp % 10;
                int fact = 1;
                for (int j = 1; j <= d; j++) {
                    fact = fact * j;
                }
                sum = sum + fact;
                temp = temp / 10;
            }
            if (sum == i) {
                System.out.print(i + " ");
            }
        }
    }
}
