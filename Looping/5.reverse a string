import java.util.*;
public class Main{
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int reverse =0;
        int temp =n;
        while(temp>0){
            int digit = temp%10;
            reverse=reverse*10+digit;
            temp=temp/10;
        }
        System.out.println(reverse);
    }
}
