import java.util.*;
public class Main{
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int prev = 10;
       boolean isfound = true;
        while(num>0){
            int  current = num%10;
            if(current>prev){
                isfound = false;
                break;
            }
            prev = current;
        num = num/10;
        }
        if(isfound){
            System.out.println("Ascending order");
        }
        else{
            System.out.println("not in ascending order");
        }
    }
}
