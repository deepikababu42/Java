import java.util.*;
public class Main{
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
            int a = sc.nextInt();
            int b = sc.nextInt();
           // int temp;
            //temp = a;
            a = a+b;
            b = a-b;
            a = a-b;
            System.out.println(a+"\n"+b);
            //System.out.println(b);
        }
    }
