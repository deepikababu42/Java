import java.util.*;
public class Main{
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
            int n= sc.nextInt();
           // int num2 = sc.nextInt();
            System.out.println(((n>0) && (n&(n-1))==0)?"Number is power of two":"Number is not a power of 2");
    }
    }
