import java.util.*;
public class Main{
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
        int a =sc.nextInt();
        int b = sc.nextInt();
        //int last1 = a%10;
        int product = a*b;
       // System.out.println(last1);
        System.out.println(product);
        if(product>=0){
            System.out.println("positive");
        }
        else{
            System.out.println("Negative");
        }
    }
}
