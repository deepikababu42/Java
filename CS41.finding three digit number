import java.util.*;
public class Main{
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
        int n =sc.nextInt();
       //int b = sc.nextInt();
       // int n = a-b;
        if(n>=100&&n<=999){
            System.out.println("It is a three digit number");
        }
        else{
            System.out.println("It is not a three digit number");
        }
    }
}
