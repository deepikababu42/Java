import java.util.*;
public class Main{
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
        double c= sc.nextDouble();
        //double celsius = (F-32)*5/9;
        //System.out.printf("%.2f\u00B0C",celsius);
      double Fahreheit = (c*9/5)+32;
        System.out.printf("%.2f\u00B0F",Fahreheit);
        
    }
    }
